#!/usr/bin/env ruby

require "google/cloud/storage"

storage = Google::Cloud::Storage.new(
	project_id: 'hebe-001',
	credentials: 'gcp.json'
)

bucket = storage.bucket 'hebe-001.appspot.com'

file = bucket.file '/tmp/bundle.tar.bz2'

file.create_file '/tmp/bundle.tar.bz2', 'cache/bundle.tar.bz2'

exit 0
