- content_for :head do
  = javascript_include_tag :upload
  
:css
  input {
    margin: 50px 10px;
  }
  a {
    display: block;
  }

%input{ type: "file", id: "Filedata", name: "Filedata[]", multiple: "multiple" }

.progress
  #progress-bar.progress-bar.progress-bar-info.progress-bar-striped{ "role" => "progressbar", "aria-valuenow" => 0, "aria-valuemin" => 0, "aria-valuemax" => 100 }

= render partial: "uploads/new.js"
