- content_for :head do
  = javascript_include_tag :layout
  = stylesheet_link_tag :layout

%div{ 'data-role': 'page' }
  .back
    .row
      %ul
        %li
          = link_to music_folders_path(search_params), default_transition do
            %i.fas.fa-4.fa-list

  - cache @music_folder do
    = render partial: 'music_folders/images'
    = render partial: 'music_folders/audio_files'
